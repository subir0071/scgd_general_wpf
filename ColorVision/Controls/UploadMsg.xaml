<Window x:Class="ColorVision.Controls.UploadMsg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.Controls"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance local:IUploadMsg}"
        Title="UploadMsg" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="{Binding Msg}"  Margin="5" FontSize="18" HorizontalAlignment="Center"/>
        <ListView Grid.Row="1" ItemsSource="{Binding UploadList}" Background="Transparent">

        </ListView>
    </Grid>
</Window>
