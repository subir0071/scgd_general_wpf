<Window x:Class="ColorVision.ProjectManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision"
        d:DataContext="{d:DesignInstance local:ProjectManager}"
        mc:Ignorable="d"
        Title="ProjectManagerWindow" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <StackPanel>
            <Button Content="创建快捷方式到桌面" Command="{Binding CreateShortCutCommand}"/>
        </StackPanel>
    </Grid>
</Window>
