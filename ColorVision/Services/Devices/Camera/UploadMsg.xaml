<Window x:Class="ColorVision.Services.Devices.Camera.UploadMsg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.Services.Devices.Camera"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance local:DeviceCamera}"
        Title="UploadMsg" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <TextBlock Text="{Binding Msg}"  HorizontalAlignment="Center"/>
    </Grid>
</Window>
