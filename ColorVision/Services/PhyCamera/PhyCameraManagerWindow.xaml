<Window x:Class="ColorVision.Services.PhyCamera.PhyCameraManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.Services.PhyCameras"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance local:PhyCameraManager}"
        Title="PhyCameraManagerWindow" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid Margin="5">
        <StackPanel>
            <Button Content="添加相机" Command="{Binding CreateCommand}"/>
        </StackPanel>
    </Grid>
</Window>
