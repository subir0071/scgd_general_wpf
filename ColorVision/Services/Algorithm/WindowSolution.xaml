<Window x:Class="ColorVision.Services.Algorithm.WindowSolution"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.Services.Algorithm"
        xmlns:draw ="clr-namespace:ColorVision.Draw;assembly=ColorVision.Util"
        xmlns:colorvison ="clr-namespace:ColorVision"
        mc:Ignorable="d"
        Title="数据查看窗口" Height="1080" Width="1920" Initialized="Window_Initialized">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="ListCol1" Width="300"/>
            <ColumnDefinition x:Name="ListCol2" Width="*"/>
        </Grid.ColumnDefinitions>
        <ListView  x:Name="ListView1" Grid.Column="0"/>
        <GridSplitter Grid.Column="1" Width="5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Black"/>
        <colorvison:ImageView Grid.Column="1" x:Name="imageview" />
    </Grid>
</Window>
