<Window x:Class="ColorVision.Wizards.WizardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.Wizards"
        mc:Ignorable="d"
        Title="WizardWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="MysqlButton" Content="Mysql配置" Click="MysqlButton_Click" />
            <Button x:Name="MQTTButton" Content="MQTT配置" Click="MQTTButton_Click" />
            <Button x:Name="RCButton" Content="RC配置" Click="RCButton_Click" />

            <Button Content="配置结束" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Window>
